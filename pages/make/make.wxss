  width: 70%;
  height: 99%;
  margin-left: 15%;
  text-align: center;
  line-height: 130rpx;
  font-size: 30rpx;
  color:#1fbc1d;
  border-bottom: 3rpx solid #1fbc1d;

}
.plate{
  width: 100%;
  height: 20%;
  margin-top: 20rpx;
  background: #fff;
  float: left;
}
.plate-left{
