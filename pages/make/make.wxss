.body{
  width: 100%;
  height: 100%;
  position: absolute;
  font-family: "微软雅黑";
  background: #f3f3f3;
}
.header{
  width: 100%;
  height: 10%;
  background: #fff;
}
.contont{
  width: 100%;
  height: 90%;
  background: #f3f3f3;
 
}

.null{
  width: 100%;
  height: 100%;
display: none;
}
.header-left{
  float: left;
  width: 50%;
  height: 100%;
}
.header-right{
   float: left;
  width: 50%;
  height: 100%;
}
.textContontgray{
  width: 70%;
  height: 99%;
  margin-left: 15%;
  text-align: center;
  line-height: 130rpx;
  font-size: 30rpx;
  color: #ccc;

}
