  line-height: 70rpx;
}
.greenBtn{
  background:#1fbc1d;
  width: 160rpx;
  height: 70rpx; 
  border-radius: 15rpx;
  margin-top: 70rpx;
  margin-left:5rpx;
  color: #fff;
  text-align: center;
  font-size: 29rpx;
  line-height: 70rpx;
}
.words_one{
  width: 100%;
  height: 45rpx;
  margin-top: 17rpx;
}
.words_two{
  width: 100%;
  height: 40rpx;
  margin-top: 4rpx;
}
.words_three{
  width: 100%;
  height: 40rpx;
  margin-top: 3rpx;
}
.words_four{
  width: 100%;
  height: 40rpx;
  margin-top: 3rpx;
}
.icon{
  width: 100rpx;
  height: 98rpx;
  background-size: cover;
  background: url(data:image/jpeg;base64,);
  position: absolute;
  top: 35%;
  left: 45%;
  
}
.null_text{
  width: 200rpx;
  height: 40rpx;
  font-size: 33rpx;
  color: #b3b3b3;
  
  position: absolute;
  top: 45%;
  left: 43%;
}
.show{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 122rpx;
  transition: 1.5s ease-out;
  background: #f3f3f3;
  transform: scale(1,1) rotate(0deg);
  
}
.hide{
   width: 100%;
  height: 100%;
  background: #f3f3f3;
   top: 122rpx;
  position: absolute;
  transition: 1.5s ease-out;
  transform: scale(0,0) rotate(45deg);
 
 
 
}
