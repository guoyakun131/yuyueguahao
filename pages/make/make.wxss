}
.words_people{
  width: 100%;
  height: 40rpx;
  margin-top: 5rpx;
}
.black{
  font-size: 30rpx;
  color: #444242;
}
.gray{
  font-size: 25rpx;
  color: #ccc;
}
.red{
  font-size: 26rpx;
  color: #f46b6a;
}
.blue{
  font-size: 26rpx;
  color: #4168bd;
}
.redBtn{
  background:#f46b6a;
  width: 160rpx;
  height: 70rpx; 
  border-radius: 15rpx;
  margin-top: 70rpx;
  margin-left:5rpx;
