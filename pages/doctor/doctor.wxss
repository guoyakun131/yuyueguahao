.body{
  width: 100%;
  height: 100%;
  position: absolute;
  font-family: "微软雅黑";
 
}
.header{
  width: 100%;
  height: 25%; 
   background: black;
   background-size: cover;
   float: left;
}

.searchAndPrompt{
  width: 100%;
  height: 30%;
  float: left;
  margin-top: 5%;
 
  
}
.inputView{
  width: 63%;
  height: 70%;
  background: white;
  border-radius: 30rpx;
 margin-left: 12%;
 float: left;
}
.input-Icon{
  width: 40rpx;
  height: 40rpx;
  float: left;
  background: url(data:image/png;base64,);
  background-size: 40rpx 40rpx;
  margin-top: 2.5%;
  margin-left: 6%;
}
.Input-view{
  height: 40rpx;
  width: 80%;
  margin-top: 1%;
  margin-left: 2.5%;
  float: left;
}
.input{
  width: 100%;
  height: 90%;
  font-size: 25rpx;
  color: #3e3f3f;
}
.Prompt{
float: left;
  width: 11%;
  height: 70%;
  margin-left: 5%;
  margin-top: 0.5%;
 
}
.PromptIcon{
  float: left;
  width: 46rpx;
  height: 55rpx;
  background: url(data:image/png;base64,);
  background-size: 46rpx 55rpx;
}
.PrimptDot{
  width: 13rpx;
  height: 13rpx;
  border-radius: 50%;
  background: #fc5555;
  float: left;
}
.header-Text{
  width: 100%;
  height: 60%;
  float: left;
}
.visit{
  width: 50%;
  height: 80%;
  margin-top: 3%;
  float: left;
  text-align: center;
}
.text{
  width: 100%;
  height: 40%;
  color: #f5f5f5;
  font-size: 30rpx;
}
.number{
  width: 100%;
  height: 40%;
  font-size: 30rpx;
  color:#fc5555;
}
.line{
  float: left;
  background: #f5f5f5;
  width: 2rpx;
  height: 50rpx;
  margin-top: 35rpx;
  margin-left: 3%;
}
.follow{
  width: 30%;
  height: 80%;
  margin-top: 3%;
  float: left;
  text-align: center;
  margin-left: 8%;
}
.contont{
    width: 100%;
    height: 75%;
   
}
.scroll-view-y{
  width: 100%;
  height: 100%;
 white-space: nowrap; 
 display: flex;

}
.expert{
  width: 750rpx;
  height: 350rpx;
  float: left;
  border-bottom: 15rpx solid #f5f5f5;
 
  
  
  
}
.expert-header{
  width: 100%;
  height: 20%;
 font-size: 30rpx;
 text-align: center;
 line-height: 70rpx;
 color: #1466a8;
  border-bottom: 3rpx #ccc solid;
}
.expert-scroll{
  width: 100%;
  height: 80%;
  margin-left: 20rpx;
}
.scroll-view-x{
  width: 100%;
  height: 100%;
 white-space: nowrap; 
 display: flex;




}
.expert-contont{
  width: 243rpx;
  height: 243rpx;
  
  display: inline-block;
  margin-top: 5%;
 
}
.imageAndname{
  width: 85%;
  height: 90%;
  text-align: center;
  font-size: 28rpx;
  color: #ccc;
  float: right;
  margin-top: 6%;
  
}

.image{
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
 
}
.image image{
  width: 100%;
  height: 100%;
  float: left;
  border-radius: 50%;
}

.name{
  width: 70%;
  height: 30%;
  line-height: 70rpx;
  
}
